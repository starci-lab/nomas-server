{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Nomas Colyseus Debug",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run", "start:debug nomas-colyseus"],
            "autoAttachChildProcesses": true,
            "restart": true,
            "sourceMaps": true,
            "stopOnEntry": false,
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "skipFiles": ["<node_internals>/**"]
        }
    ]
}
